#!/bin/sh
:

GT_VERSION=1.0.1

###### 全局设置，不同的脚本请更改这里
GT_MP="$HOME/issuehq_quotation"
GT_SERVICE_NAME="<投资品行情采集服务核心>"

###### 获取脚本名称，选项，当前路径等信息
GT_PROPATHNAME=$0
GT_PRONAME=${GT_PROPATHNAME##*/}
GT_PROPID=$$
GT_OPTION=$1
GT_OPTION2=$2

CURRENT_PATH=$PWD

#### 加载GNNT函数库和配置项
[ -e $HOME/bin/gnnt_profile ] && . $HOME/bin/gnnt_profile
[ -e $HOME/bin/gnnt_lib ] && . $HOME/bin/gnnt_lib

###### 检查 - 部署目录
gnnt_check_mp

#### 检查环境
gnnt_check_env

## 准备搜索的关键字
GT_KEY_WORD="QuotationIssueServerShell"
	
###### 脚本主过程

## 运行主脚本前，检查用户设置 
gnnt_check_setting

gnnt_main 
