<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="gnnt.MEBS.espot.front.model.trade.GoodsMoneyApply" table="E_goodsmoneyapply" dynamic-insert="true" dynamic-update="true">
		<id name="id">
			<generator class="native">
				<param name="sequence">SEQ_E_GOODSMONEYAPPLY</param>
			</generator>
		</id>
		<many-to-one name="tradeContract" column="tradeNo" class="gnnt.MEBS.espot.front.model.trade.Trade"/>
		<property name="applyTime" >
			<column name="createTime"></column>
		</property>
		<property name="processTime" />
		<property name="status" />
		<property name="type" />
	</class>
</hibernate-mapping>